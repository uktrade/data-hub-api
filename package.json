{
    "dependencies": {
        "govuk-frontend": "5.9.0",
        "sass": "1.86.3"
    },
    "scripts": {
        "css:build": "sass datahub/core/static/core/stylesheets/datahub.scss:datahub/core/static/core/stylesheets/datahub.css --style=compressed --source-map --load-path=nodule_modules/",
        "css:watch": "sass datahub/core/static/core/stylesheets/datahub.scss:datahub/core/static/core/stylesheets/datahub.css --style=compressed --source-map --watch --load-path=nodule_modules/",
        "assets:build": "mkdir -p static/assets && cp -r node_modules/govuk-frontend/dist/govuk/assets/* static/assets/",
        "build": "npm run assets:build && npm run css:build"
    },
    "engines": {
        "node": "23.11.0",
        "npm": "11.3.0"
    }
}
