[format]
max-line-length=99
ignore = datahub.*.migrations,manage.py,config.settings.*
; C0103: Module name <foo> doesn't conform to snake_case naming style (invalid-name)
; C0114: Missing module docstring (missing-module-docstring)
; C0115: Missing class docstring (missing-class-docstring)
; E1101: Instance of '_MPTTObjectName' has no 'parent' member (no-member)
;  * This is often incorrect for dynamically generated fields e.g. django ORM models `objects`
; R0801: Similar lines in 2 files
; R0903: Too few public methods (<n>/2) (too-few-public-methods)
; R0901: Too many ancestors (<n>/7) (too-many-ancestors)
; W0511: TODO: <todo> (fixme)

disable=C0103,C0114,C0115,E1101,R0801,R0901,R0903,W0511,
# vim: ft=dosini
